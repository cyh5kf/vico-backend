  "scripts": {
    "clean": "rimraf ./client/dist",
    "dll": "webpack --config ./client/webpack_build/webpack.config.dll.js --progress --colors",
    "build-dev": "webpack-dev-server --config ./client/webpack_build/webpack.config.dev.js --progress --colors",
    "build": "npm run clean && npm run dll && webpack --config ./client/webpack_build/webpack.config.prod.js --progress --profile --colors",
    "node-vscode-debug": "cross-env NODE_ENV=development node --debug-brk --nolazy ./server/app.js",
    "node-vscode-debug-build": "cross-env NODE_ENV=production node --debug-brk --nolazy ./server/app.js",
    "node-chrome-debug": "cross-env NODE_ENV=development nodemon --inspect --debug-brk --ignore ./client --watch ./server ./server/app.js",
    "node-dev": "cross-env NODE_ENV=development nodemon --ignore ./client --watch ./server ./server/app.js",
    "node-build": "cross-env NODE_ENV=production node ./server/app.js",
    "pm2": "cross-env NODE_ENV=production pm2 start processes.json"
  },
